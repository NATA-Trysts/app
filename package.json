{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "format": "prettier --write src//**/*.{ts,tsx,css} --config ./.prettierrc.json",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint",
      "npm run lint:fix",
      "npm run format"
    ]
  },
  "dependencies": {
    "@100mslive/react-sdk": "^0.6.3",
    "@fvilers/disable-react-devtools": "^1.3.0",
    "@radix-ui/react-popover": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.0",
    "@radix-ui/react-switch": "^1.0.1",
    "@react-three/drei": "^9.45.0",
    "@react-three/fiber": "^8.9.1",
    "@react-three/rapier": "^0.13.2",
    "@sonhaaa/3d-playground": "^0.0.20",
    "axios": "^1.3.4",
    "colyseus.js": "^0.14.13",
    "framer-motion": "^9.0.7",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.4",
    "react-pin-input": "^1.3.0",
    "react-router-dom": "^6.8.1",
    "react-textarea-autosize": "^8.4.0",
    "react-tooltip": "^5.8.1",
    "styled-components": "^5.3.6",
    "three": "^0.146.0",
    "three-stdlib": "^2.21.8",
    "timeago.js": "^4.0.2",
    "vite-plugin-svgr": "^2.4.0",
    "zustand": "^4.3.2"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.8.4",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}
